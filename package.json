{
  "name": "eddieodozi-node-with-react-build-deploy-a-fullstack-web-application",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "start": "nodemon ./build/app.js",
    "dev:mac": "nodemon -r tsconfig-paths/register src/app.ts | ./node_modules/.bin/bunyan",
    "dev": "nodemon -r tsconfig-paths/register src/app.ts | bunyan",
    "lint:check": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx}",
    "prettier:check": "prettier --check 'src/**/*.{ts,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,json}'",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json"
  },
  "keywords": [],
  "author": "Rasim Aghayev <rasimaqayev@gmail.com>",
  "license": "ISC",
  "description": "Build & deploy a social network app using React, Typescript, MongoDB, Redis, Queues, AWS, Terraform, CircleCI & more.",
  "dependencies": {
    "@socket.io/redis-adapter": "^8.3.0",
    "bunyan": "^1.8.15",
    "compression": "^1.7.4",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.3.0",
    "mongoose": "^8.5.1",
    "redis": "^4.6.15",
    "socket.io": "^4.7.5",
    "typescript": "^5.5.4",
    "typescript-transform-paths": "^3.4.7"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.11",
    "@types/compression": "^1.7.5",
    "@types/cookie-session": "^2.0.49",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.6",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0"
  }
